// @todo refactor to the routes config so all routes are defined in one place
const <%= route.name %> = {
  route: '<%= route.route %>',
  view: '<% if (view && view.name) { %><%= view.name %><% } else { %><%= route.view %><% } %>',
  middleware: {
    security: [<% for (var i = 0; i < route.middleware.security.length; i++) { %>
      '<%= route.middleware.security[i]%>'<% if (i < route.middleware.security.length - 1) { %>,<% } %><%}%>
    ],
    data: [<% for (var i = 0; i < route.middleware.data.length; i++) { %>
      '<%= route.middleware.data[i]%>'<% if (i < route.middleware.data.length - 1) { %>,<% } %><%}%>
    ]
  },
  unauthorized: '<%= route.unauthorized %>'
};

export default <%= route.name %>;